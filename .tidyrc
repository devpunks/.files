output-file: /dev/null

indent: auto
doctype: omit
newline: CRLF
quote-marks: no
show-errors: 1
indent-spaces:2
doctype: html5
uppercase-tags: no
show-warnings: yes
input-encoding: utf8
accessibility-check: 0

mute: INSERTING_TAG
mute: MISSING_DOCTYPE
mute: MISSING_TITLE_ELEMENT
mute: MISSING_QUOTEMARK_OPEN
mute: MISSING_ENDTAG_OPTIONAL

